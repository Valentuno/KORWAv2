        <Button x:Name="Zmiana_Copy1" Content="Page3" Margin="709,642,135,0" VerticalAlignment="Top" Background="#FFC11023" BorderBrush="#FFAF7E3F" Foreground="White" FontFamily="Gill Sans MT" FontSize="20" FontWeight="Bold" Height="43" Click="Zmiana_Click" RenderTransformOrigin="3.896,0.455" Grid.Column="3"/>

    </Grid>
