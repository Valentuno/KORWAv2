        xmlns:local="clr-namespace:pages"
        mc:Ignorable="d"
        Title="MainWindow" Height="768" Width="1024">
    <Grid Width="1024" Height="768">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="0*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="Obiekt" Content="Frame" HorizontalAlignment="Right" Width="1024" Background="#FFEAEAEA" Grid.Column="1" Grid.ColumnSpan="2" Height="568" VerticalAlignment="Top"/>

        <Rectangle Grid.ColumnSpan="3" HorizontalAlignment="Left" Height="3" Margin="0,735,0,0" Stroke="Black" VerticalAlignment="Top" Width="1014" Fill="White"/>
